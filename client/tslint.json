{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "arrow-parens": false,
        "no-console": [true, "log"],
        "no-empty": false,
        "interface-name": false,
        "max-classes-per-file": false,
        "no-bitwise": false
    },
    "rulesDirectory": []
}